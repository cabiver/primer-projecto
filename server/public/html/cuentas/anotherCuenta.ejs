</div> -->

<div class="containerUser">
    <div class="difuminado-background background-height">
        <%- backgroundDifuminado %>
    </div>
    <div id="Main-cuentas.js-para_posicionar_la_camara_del_background">

        


    </div>


    <div class="background background-height" id="Mains.js-para_hacer_margin_top">

        <div class="div-image-background" id="div-image-background">

            <%- extencion %>


        </div>


    </div>

  
    <div class="FLEXperfilUser" id="FLEXperfilUser">
        <div class="FLEXBOX ALIGN-ITEMS boxUser">

            <div class="icono-press iconPerfil MainsJS-arrays_de_iconos_para_volverlos_cuadrados" id="Main-cuentas.js-operador_para_posicionar_la_camara-icono">

                <img class="iconoPrincipal" src="<%= perfil %>" alt="">
            </div>
            <div class="perfilName">
                <%= user %>
            </div>
        </div>
        <div class="botton_agregar_persona" id="main-another-cuenta.js-agregar_persona_amigo"><%=amigo?"dejar de segir":"agregar a amigos"%></div>
    </div>
   

</div>

<div class="bak" id="full">


    <div class="marco" id="Mains.js-div-donde-colocaras-los-post">
    
    
    
    </div>
    <div class="FLEXBOX JUSTIFY-CONTENT">
        <div class="div-para-marco FLEXBOX ALIGN-ITEMS" id="mains.js-limite-fotos" style="display: none;">
            <p class=" fuente">ya no hay mas fotos?</p>
        </div>
    </div>
    
    
    <div id="Mains.js-detectar-cuando-es-observado" style="height: 15px;"></div>
</div>

<script src="scripts/cuentas/main-another-cuenta.js"></script>

