<div class="containerUser">
    <div class="difuminado-background background-height">
        <%- backgroundDifuminado %>
    </div>
    <div id="Main-cuentas.js-para_posicionar_la_camara_del_background" class="divrCamaraIcono">

        <div id="Main-cuentas.js-comparador_de_tamaÃ±o_para_posicionarlo" class="divrCamaraIcono--div-background">
                <div class="divrCamaraIcono__plus-camara-div">
                    <img class="divrCamaraIcono__img" src="images/mas.png" alt="">
                </div>
                <div class="camara-background feedCamara">
                    <img class="imagen-de-camara" id="changeBackground" src="images/camara.png" alt="">
                </div>
        </div>


    </div>


    <div id="Mains.js-para_hacer_margin_top" class="background background-height">

        <div id="div-image-background" class="div-image-background">

            <%- extencion %>


        </div>


    </div>

    <div id="Main-cuentas.js-dezplegar-form-cambiar-icono" class="divrCamaraIcono">
        <div class="divrCamaraIcono__plus-camara-div">
            <img class="divrCamaraIcono__img" src="images/mas.png" alt="">
        </div>
        <div class="camara-background-icono feedCamara">
            <img class="imagen-de-camara" id="changeBackground" src="images/camara.png" alt="">
        </div>
    </div>

    <div id="FLEXperfilUser" class="FLEXperfilUser">
        <div class="FLEXBOX ALIGN-ITEMS boxUser">

            <div id="Main-cuentas.js-operador_para_posicionar_la_camara-icono" class="icono-press iconPerfil MainsJS-arrays_de_iconos_para_volverlos_cuadrados">

                <img class="iconoPrincipal" src="<%= perfil %>" alt="">
            </div>
            <div class="perfilName">
                <%= user %>
            </div>
        </div>

    </div>
    <form class="superponer text-claro change-icon" action="" style="display: none;" id="main-cuentas.js-formulario_para_cambiar_icono">
        <div class="flexCenter">
            <input id="iconImag" name="image" type="file" style="display: none;">
            <label class="text-change-icon " for="">quieres cambiar el icono de tu perfil?</label>
            <label for="iconImag" class="buttonLable">subir icono</label>
            <img id="main-cuentas-js-renderizar-icono" class="renderIcono" src="" alt="">
            <label id="resultIcono" class="resultIcono" for=""></label>
            <button class="enviarIcon ">enviar icono</button>
        </div>

    </form>

</div>

<div id="full" class="bak">

    <div class="FLEXBOX JUSTIFY-CONTENT">

        <div class="container">
            <form id="main-cuentas.js-formulario_subir_post" class="form-img" enctype="multipart/form-data">
                <input id="main-cuentas.js-detectar-cambio-de-imagen" type="file" name="image" style="display: none;">
                <label for="imgFile" class="buttonLable ">sube una imagen</label>
                <div id="main-cuentas.js-div_renderisa_pre_prost" class="load-imge">
                    <div id="barra-de-carga"></div>
                    <video id="loadVideo" class="render-de-video pc" src=""style="width: 0;"></video>
                    <img id="main-cuentas.js-imagen_para_renderiar" src="" alt="" class="render-de-imagen precentacion image pc">
                </div>
                <video id="loadVideoMobile" class="render-de-video-mobile" src=""></video>
                <img id="imagMobile" src="" alt="" class="render-de-image-mobile image mobile">
               
                <p class="INLINE-BLOC">agrega una description</p> 
                 <input  class="desc" autocomplete="off" type="text" name="description"> 
                <button class="sendbutton">subir Post</button>
            </form>
    
        </div>
    </div>
    
    <div id="Mains.js-div-donde-colocaras-los-post" class="marco">
    
    </div>
    <div class="FLEXBOX JUSTIFY-CONTENT">
        <div id="mains.js-limite-fotos" style="display: none;" class="div-para-marco FLEXBOX ALIGN-ITEMS">
            <p class=" fuente">ya no hay mas fotos?</p>
        </div>
    </div>
    
    <div id="Mains.js-detectar-cuando-es-observado" style="height: 15px;"></div>
</div>

<script src="scripts/cuentas/main-cuentas.js"></script>


