<header id="Header">

    <body>
        <div class="gridChangeBackground" id="main-cuentas.js-detectar-click-para-cambiar-background" style="display: none;">
            <div class="div-cerrar" id="main-cuentas.js-detectar-click"> 
                <img class="div-cerrar__x" src="images/close-x.png" alt=""> 
            </div>

            <div class="div-change-center-background">
                <form class="div-change-center-background__form" id="main-cuentas.js-form-del-background" action="">
                    <input id="backgroundFile" type="file" style="display: none;" name="image">
                    <label for="">cambia de fondo aqui</label>
                    <label class="buttonLable" for="backgroundFile">subir</label>
                    <img class="div-change-center-background__render-background" id="main-cuentas.js-renderizar-background" src="" alt="">
                    <button>enviar fondo</button>
                    <label id="main-cuentas.js-respuesta-de-la-peticion"></label>
                </form>
            </div>

        </div>

        <div class="cabecera" id="Mains.js-calcular_y_asignar_width_a_la_cabecera_y_detectar_click">
            <div id="Mains.js-calcular_distancia_y_esperar_click">
                <div  class="separadorDeCabecera" id="iconoDeLaPagina">
                    <img class="icono js-detectar_cuando_carge" src="images/camille-300x300.png" alt="">
                </div>
            </div>
            <div id="Mains.js-referencia_para_calcular_la_pocision_de_la_lupa-barra_de_busqueda-div_barra_busqueda">
                <form id="Mains.js-variable_para_el_posicionamiento_de_la_lupa-form_busqueda" class="barraDeBusqueda" >
                    <input id="Mains.js-variable_de_posicionamiento" autocomplete="off" name="usuarios" class="inputs-text-sin-mamadas div-dentro-de-barra-de-busqueda" action="" type="text">
                    <button id="Main.js-posicionar_la_lupa" class="fit-height">
                        <img id="lupa" class="icono-lupa" src="images/lupa.png" >
                    </button>
                  
    
                </form>
                <div style="display: flex; justify-content: center;">
                    <div id="Mains.js cuentas.js-posicionamiento_y_actualizacion_de_peticiones_de_recomendados" class="divRecomiendaciones"></div>
                </div>
                
            </div>

                <div id="Mains.js-div_de_tu_perfil_para_ajustar_barra_de_busqueda" class="div-perfil-tuyo">
                    <div class="separadorDeCabecera div-icono-de-usuario" id="Mains.js-agregar_eventos_que_desplegue_opciones_de_usuarios">
                        <div class="MainsJS-arrays_de_iconos_para_volverlos_cuadrados iconPerfil">
                            <img class="iconoPrincipal"  src="<%= icon %>">
                        </div>
                        
                        <div class="nombre">
                            <%= name.length>7?`${name.substr(0,7)}...`:name %>
                        </div>
                    </div>
        
                </div>
            
            
        </div>

        <div id="Mains.js-pesplegar-opciones-de-sesion" class="Flex-navegador-user" style="display: none;">
            <button id="Mains.js-detectar-click-ir-a-tu-sitio-web" class="p-de-navegador">Ir a mi sitio web</button>
            <button id="Mains.js-detectar-click-eliminar-cookies" class="p-de-navegador">cerrar Sesion</button>
        </div>

</header>

